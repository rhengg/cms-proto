{
	"name": "directus-extension-customhook",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-hook"
	],
	"directus:extension": {
		"type": "hook",
		"path": "./index.js",
		"source": "src/index.js",
		"host": "^9.2.2"
	},
	"scripts": {
		"build": "directus-extension build",
		"watch": "nodemon --watch src --exec \" npm run build \" "
	},
	"devDependencies": {
		"@directus/extensions-sdk": "9.2.2",
		"nodemon": "^2.0.15"
	}
}
